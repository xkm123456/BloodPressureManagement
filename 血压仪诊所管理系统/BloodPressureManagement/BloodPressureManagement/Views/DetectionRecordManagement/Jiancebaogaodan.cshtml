@model List<BloodPressureManagement.Models.Projects>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>

<head>
    <meta charset="UTF-8">
    <title>sinocares</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,user-scalable=yes, minimum-scale=0.4, initial-scale=0.8,target-densitydpi=low-dpi" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="~/css/font.css">
    <link rel="stylesheet" href="~/css/xadmin.css">
    <script src="~/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/layui/layui.js" charset="utf-8"></script>
    <script type="text/javascript" src="~/js/xadmin.js"></script>
    <!-- 让IE8/9支持媒体查询，从而兼容栅格 -->
    <!--[if lt IE 9]>
      <script src="https://cdn.staticfile.org/html5shiv/r29/html5.min.js"></script>
      <script src="https://cdn.staticfile.org/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body class="layui-anim layui-anim-up">
    <div class="layui-row layui-bg-gray">
        <br />

        <form class="layui-form layui-col-md12 x-so" style="width: 1200px;">
            <div class="layui-inline">
                <label for="L_pass" class="layui-form-label" style="font-size: 14px;width: 200px;"><b><span>@ViewBag.name</span>/<span>@ViewBag.gender</span>/<span>@ViewBag.age</span>岁/<span>@ViewBag.height</span>cm/<span>@ViewBag.weight</span>kg</b></label>

            </div>

        </form>
        <div style="margin-left: 1100px; ">
            <button class="layui-btn" lay-submit="" lay-filter="sreach"><a href="/DetectionRecordManagement/Examiner_Add">检测下一位</a></button>
            <button class="layui-btn" lay-submit="" lay-filter="sreach"><a href="/DetectionRecordManagement/Jiancejilu">历史记录</a></button>


        </div>
    </div>
    <table class="layui-table">

        <thead>
            <tr>
                <th>
                    <div class="layui-unselect header layui-form-checkbox" lay-skin="primary"><i class="layui-icon">&#xe605;</i></div>
                </th>
                <th>序号</th>
                <th>项目名称</th>
                <th>单位</th>
                <th>收缩压</th>
                <th>舒张压</th>
                <th>参考值</th>
                <th>校验方法</th>
                <th>结果解读</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    <div class="layui-unselect layui-form-checkbox" lay-skin="primary" data-id='2'><i class="layui-icon">&#xe605;</i></div>
                </td>
                <td>@item.Did</td>
                <td>@item.Util</td>
                <td>@item.Dname</td>
                <td>@item.ShouSuoYa</td>
                <td>@item.ShuZhangYa</td>
                @* 参考值 *@
                <td>
                    1.正常收缩压[90-140]mmHg  2.正常舒张压[60-90]mmHg
                </td>
                <td>@item.Method</td>
                <td>
                    @item.Decipher
                </td>
            </tr>
            }
        </tbody>
    </table>
    <script>

      layui.use('laydate', function(){
        var laydate = layui.laydate;

        //执行一个laydate实例
        laydate.render({
          elem: '#start' //指定元素
        });

        //执行一个laydate实例
        laydate.render({
          elem: '#end' //指定元素
        });
      });


    </script>
</body>

</html>
